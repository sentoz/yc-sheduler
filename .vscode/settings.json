{
  "go.lintTool": "golangci-lint",
  "go.lintFlags": [
    "--fast-only",
    "--output.text.print-issued-lines=false"
  ],
  "go.testEnvFile": "${workspaceFolder}/.env",
  "cSpell.ignorePaths": [
    "go.mod",
    "go.sum",
    ".github/actions/**/*.yaml",
    ".github/workflows/**/*.yaml",
    ".vscode/*"
  ],
}
